# Configuration API Telegram
# Obtenez ces valeurs depuis https://my.telegram.org/auth
TELEGRAM_API_ID=votre_api_id_ici
TELEGRAM_API_HASH=votre_api_hash_ici
TELEGRAM_SESSION_NAME=clonage_telegram

# Configuration Bot (optionnel)
# Créez un bot avec @BotFather et ajoutez-le comme admin du canal cible
TELEGRAM_BOT_TOKEN=votre_bot_token_ici
USE_BOT_FOR_SENDING=false

# Configuration de limitation de taux
RATE_LIMIT_DELAY=1.0
BATCH_SIZE=10
MAX_RETRIES=3
RETRY_DELAY=5.0

# Configuration de journalisation
LOG_FILE=clonage_telegram.log
LOG_LEVEL=INFO

# Configuration de suivi de progression
PROGRESS_FILE=progression_clonage.json
SAVE_PROGRESS_INTERVAL=50

# Configuration des médias
DOWNLOAD_MEDIA=true
MEDIA_TIMEOUT=300
